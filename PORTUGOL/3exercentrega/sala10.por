programa
{
	
	funcao inicio()
	{
		real C
		real F
		
		escreva("Digite a temperatura em Celsius: ")
		leia(C)

          F = (C*1.8)+32
		
	     escreva("A temperatura em Fahrenheit é: ", F)
	}
}
